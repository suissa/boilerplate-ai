{
  "name": "boilerplate-ai",
  "version": "1.0.0",
  "description": "Monorepo Nx com React + NestJS - Sistema de autenticação por código",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "check-versions": "node check-versions.js",
    "setup": "npm run check-versions && echo '✅ Setup verificado com sucesso!'",
    "dev": "npx nx run-many --target=serve --all",
    "dev:frontend": "npx nx serve frontend",
    "dev:backend": "npx nx serve backend",
    "build": "npx nx run-many --target=build --all",
    "build:frontend": "npx nx build frontend",
    "build:backend": "npx nx build backend",
    "test": "npx nx run-many --target=test --all",
    "test:frontend": "npx nx test frontend",
    "test:backend": "npx nx test backend",
    "lint": "npx nx run-many --target=lint --all",
    "lint:fix": "npx nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "npx tsc --noEmit",
    "clean": "npx nx reset && rm -rf dist build .nx",
    "env:dev": "cp env.development .env",
    "env:test": "cp env.test .env",
    "env:prod": "cp env.production .env",
    "validate:env": "node -e \"require('./config/env.config.ts').validateConfig()\"",
    "preinstall": "node preinstall.js"
  },
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "monorepo",
    "nx",
    "react",
    "nestjs",
    "typescript",
    "authentication",
    "email-code"
  ],
  "author": "Rasta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rastamansp/boilerplate-ai.git"
  },
  "bugs": {
    "url": "https://github.com/rastamansp/boilerplate-ai/issues"
  },
  "homepage": "https://github.com/rastamansp/boilerplate-ai#readme",
  "devDependencies": {
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/devkit": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/nest": "^21.2.3",
    "@nx/node": "21.2.3",
    "@nx/playwright": "21.2.3",
    "@nx/react": "^21.2.3",
    "@nx/vite": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/webpack": "21.2.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "^24.0.13",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.2.3",
    "prettier": "^3.6.2",
    "semver": "^7.5.4",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "npm@10.0.0",
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.6.0",
    "dotenv": "^17.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  }
}